<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.trv13.ProjectWithMaven.Address.Address" table="Address">
		<id name="addressId" column="address_id">
			<generator class="native"/>
		</id>
		<property name="isOpen" column="is_open" type="boolean"/>
		<property name="location" column="add_location" type="string"/>
		<property name="city" column="add_city" type="string"/>
		<property name="date" column="add_date" type="java.util.Date"/>
		<property name="image" column="add_image" type="binary" length="2500000"/>
	</class>
</hibernate-mapping>